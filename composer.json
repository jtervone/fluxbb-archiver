{
    "name": "jtervone/fluxbb-archiver",
    "description": "Export a FluxBB forum to static HTML and JSON files",
    "type": "project",
    "license": "MIT",
    "require": {
        "php": ">=7.4",
        "ext-mysqli": "*",
        "ext-mbstring": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.6"
    },
    "autoload": {
        "psr-4": {
            "FluxbbArchiver\\": "src/FluxbbArchiver/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "FluxbbArchiver\\Tests\\": "tests/"
        }
    },
    "bin": [
        "bin/fluxbb-archiver"
    ],
    "scripts": {
        "test": "phpunit"
    }
}
